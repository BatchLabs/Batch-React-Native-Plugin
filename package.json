    "gh-pages": "^2.0.1",
    "jest": "^27.3.1",
    "prettier": "^1.14.2",
    "react-native": "^0.66.1",
    "ts-jest": "^27.0.7",
    "typedoc": "^0.22.7",
    "typescript": "^4.4.4"
  }
}